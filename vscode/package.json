{
    "name": "stepsize",
    "displayName": "Stepsize",
    "description": "Stepsize Plugin",
    "version": "0.0.1",
    "publisher": "Stepsize",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
      "*"
    ],
    "contributes": {
      "commands": [{
          "command": "extension.send",
          "title": "Stepsizzle"
      }],
      "keybindings": [
          {
            "command": "extension.send",
            "key": "ctrl+s",
            "mac": "cmd+s"
          }
      ]
    },
    "main": "./extension",
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}
